<VirtualHost *:80>
    ServerName beta.lagrummet.se
    ServerSignature On

    DocumentRoot /var/www/
    <Directory />
        Options FollowSymLinks
        AllowOverride None
    </Directory>
    <Directory /var/www/>
        Options Indexes FollowSymLinks MultiViews
        AllowOverride None
        Order allow,deny
        allow from all
    </Directory>

    ProxyPassMatch ^/robots\.txt$ !
    ProxyPass /lagrummet/ ajp://localhost:8009/lagrummet/
    ProxyPass / ajp://localhost:8009/lagrummet/

    ServerAdmin rinfo@lagrummet.se

    ErrorLog /var/log/apache2/error.log
    LogLevel warn
    CustomLog /var/log/apache2/access.log combined

</VirtualHost>
